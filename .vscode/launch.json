    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "CIFAR-10: Alexnet Profile",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/cifar.py",
            "console": "integratedTerminal",
            "args": [
                "--mode",
                "profile",
                "-a",
                "alexnet",
                "--checkpoint",
                "checkpoints/cifar10/alexnet/1",
                "--workers",
                "0"
            ]
        }
    ]
}